{
  "name": "eslint-config-triple",
  "version": "0.5.8",
  "license": "MIT",
  "description": "Unified ESlint config for Triple projects",
  "homepage": "https://www.wearetriple.com/",
  "keywords": [
    "eslint",
    "eslintconfig",
    "prettier"
  ],
  "scripts": {
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": "prettier --check"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-config-airbnb-typescript": "*",
    "eslint-config-prettier": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-only-warn": "*",
    "eslint-plugin-prettier": "*"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4"
  }
}
